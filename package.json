{
  "name": "nodeflow",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"cross-env NODE_ENV=development NODE_OPTIONS=--import=./instrument.server.mjs vite dev --port 3000\" \"bunx convex dev\"",
